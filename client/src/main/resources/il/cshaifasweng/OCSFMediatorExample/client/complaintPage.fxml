<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="il.cshaifasweng.OCSFMediatorExample.client.ComplaintController">
    <children>
        <AnchorPane layoutX="41.0" layoutY="3.0" prefHeight="400.0" prefWidth="521.0">
            <children>
                <TextArea fx:id="complaintTextArea" layoutX="169.0" layoutY="259.0" prefHeight="73.0" prefWidth="282.0" promptText="Write complaint here" />
                <Label alignment="CENTER" layoutX="120.0" layoutY="14.0" prefHeight="32.0" prefWidth="236.0" style="-fx-font-style: italic; -fx-font-weight: bold;" text="Complaint" textFill="ORANGE">
                    <font>
                        <Font size="24.0" />
                    </font>
                </Label>
                <Label fx:id="messsageLabel" layoutX="94.0" layoutY="340.0" prefHeight="25.0" prefWidth="262.0" text="Can't send an empty complaint!" textFill="#e10e0e" visible="false" />
                <TextField fx:id="idText" layoutX="80.0" layoutY="95.0" prefHeight="25.0" prefWidth="148.0" />
                <Label layoutY="90.0" prefHeight="35.0" prefWidth="87.0" text="Write your ID" />
                <Label layoutX="25.0" layoutY="259.0" prefHeight="35.0" prefWidth="131.0" text="Write your complaint" />
                <Button fx:id="submitComplaintButton" layoutX="384.0" layoutY="341.0" mnemonicParsing="false" onAction="#submitComplaintOnAction" text="Send Complaint" />
              <Button fx:id="backButton" layoutX="23.0" layoutY="341.0" mnemonicParsing="false" onAction="#back_func" text="Back" />
            <Label layoutX="240.0" layoutY="90.0" prefHeight="35.0" prefWidth="116.0" text="Write your full name" />
            <TextField fx:id="nameText" layoutX="350.0" layoutY="95.0" prefHeight="25.0" prefWidth="164.0" />
            <Label layoutY="142.0" prefHeight="35.0" prefWidth="101.0" text="Write your email" />
            <TextField fx:id="emailText" layoutX="92.0" layoutY="147.0" prefHeight="25.0" prefWidth="245.0" />
            </children>
        </AnchorPane>
    </children>
</AnchorPane>
